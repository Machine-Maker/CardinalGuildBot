{
  "env": {
      "es6": true,
      "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "linebreak-style": ["off"],
      "quotes": [
          "error",
          "single"
      ],
      "semi": "off",
      "no-console": "off"
  }
}
